import math
n = int(input("Enter any non negative number :"))
count=0
m=int(math.sqrt(n))
for i in range(m+1):
	for j in range(m+1):
		A = (i*i) + (j*j)
		if A==n and i<=j:
			print("Pair of integer is :",i,j)
			count+=1
if count == 0:
	print("Number does not relate any pair satisfying Pythagoras theorem")
		
